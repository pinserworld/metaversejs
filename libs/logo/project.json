{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/logo/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/logo",
        "main": "libs/logo/src/index.ts",
        "tsConfig": "libs/logo/tsconfig.lib.json",
        "assets": ["libs/logo/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/logo/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/logo"],
      "options": {
        "jestConfig": "libs/logo/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["library", "logo"]
}
